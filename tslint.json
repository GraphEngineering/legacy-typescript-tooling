{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": false,
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "no-unused-variable": [
      true,
      {
        "ignore-pattern": "^_"
      }
    ]
  }
}