{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false,
    "variable-name": [true, "allow-leading-underscore"],
    "no-unused-variable": [
      true,
      {
        "ignore-pattern": "^_"
      }
    ]
  }
}
