{
  "name": "typescript-tooling",
  "description": "This package helps keep TypeScript configuration and tooling synced up between seperate projects.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cruhl/typescript-tooling.git"
  },
  "keywords": [
    "typescript",
    "tslint",
    "prettier",
    "tooling"
  ],
  "author": "cruhl",
  "license": "MIT",
  "homepage": "https://github.com/cruhl/typescript-tooling#readme",
  "bugs": {
    "url": "https://github.com/cruhl/typescript-tooling/issues"
  },
  "files": [
    "bin",
    "dist",
    "tslint.json"
  ],
  "main": "dist/index.js",
  "bin": {
    "tst": "bin/cli.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "nodemon",
    "build": "tsc && cp src/DefaultConfigs/*.json dist/DefaultConfigs/",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.0"
  },
  "dependencies": {
    "caporal": "^0.10.0",
    "chalk": "^2.4.1"
  }
}
